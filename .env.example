# AI Provider API Keys Configuration
# Copy this file to .env and fill in your actual API keys

# OpenAI Configuration
OPENAI_API_KEY=sk-your-openai-api-key-here
# Optional: Custom API endpoint for OpenAI-compatible services
# OPENAI_API_BASE=https://api.openai.com/v1

# Google Gemini Configuration
GOOGLE_API_KEY=your-google-api-key-here

# Anthropic Claude Configuration
ANTHROPIC_API_KEY=sk-ant-your-anthropic-api-key-here

# Artifact Signing Configuration (already configured)
AI_ARTIFACT_SIGNING_KEY=keys\dev_private.pem
AI_ARTIFACT_KEY_ID=dev-local-user
AI_ARTIFACT_VERIFY_KEY=keys\dev_public.pem

# Optional: Model Preferences
DEFAULT_MODEL=gpt-4
DEFAULT_TEMPERATURE=0.7
DEFAULT_MAX_TOKENS=2048

# Development Settings
DEBUG=false
LOG_LEVEL=INFO
